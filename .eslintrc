{
 "parser": "babel-eslint",
 "extends": [
  "eslint:recomended",
  "plugin:react/recomended"
 ],
 "env": {
   "browser": true,
   "node": true,
   "es6": true
 },
 "rules": {
   "no-use-before-define":"warn",
   "spaced-comment":"warn"
 },
 "settings": {
   "react":{
     "version": "detect"
   }
 }
}
